{
  "name": "trevaka-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "type-check": "tsc -b .",
    "e2e-playwright": "jest --testTimeout 60000 --runInBand src/e2e-playwright/specs"
  },
  "dependencies": {
    "e2e-test-evaka-common": "link:../evaka/frontend/src/e2e-test-common",
    "e2e-playwright-evaka": "link:../evaka/frontend/src/e2e-playwright"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "jest-circus": "^27.0.6",
    "jest-junit": "^12.2.0",
    "playwright": "1.13.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.1.3"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "projects": [
      "./src/e2e-playwright"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/assetsTransformer.js"
    }
  }
}
